<%- include ('_layouts/header') -%>

<div class="container">
<center>  <h2>Log in</h2></center>
<form method="post" action="/users/login" style="text-align:center">
    <div class="row">
    <div class="form-group col-md-3">
        <!-- <label>Username</label> -->
        <input type="text" name="username" class="form-control" placeholder="Username"   style="padding: 20px 9px;">
    </div>
</div>

<div class="row">
    <div class="form-group col-md-3">
        <!-- <label>Password</label> -->
        <input type="password" name="password" class="form-control"  placeholder="Password" style="padding: 20px 9px;">
    </div>
</div>
    <button type="submit" class="btn btn-default btn-info">Log in</button>
</form>
</div>

<%- include ('_layouts/footer') -%>